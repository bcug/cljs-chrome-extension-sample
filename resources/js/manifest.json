{
  "name": "Clojurifier",
  "version": "0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
 "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["clojurifier.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}

